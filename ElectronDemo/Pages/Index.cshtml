@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<div class="table-responsive">
<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">First</th>
        <th scope="col">Last</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Processes)
    {
        <tr>
            <th scope="row">@item.Id</th>
            <td>@item.ProcessName</td>
            <td>@item.WorkingSet64</td>
        </tr>
    }
    </tbody>
</table>
</div>